{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "ID of the deleted file."
    },
    "type": {
      "type": "string",
      "enum": ["file_deleted"],
      "description": "Deleted object type. For file deletion, this is always 'file_deleted'.",
      "default": "file_deleted"
    }
  },
  "required": ["id", "type"]
}
